all_systems = [
  {
    /* Required Parameters */
    hostname               = "tcnhq-jnk01"
    region                 = "us-iso-west-1"
    availability_zone      = "us-iso-west-1c"
    # TODO: This should be automated based on availablility zone.
    subnet_id              = "subnet-000a0000000000aaa"
    key_name               = "example-admin-int"
    aws_kms_alias          = "aws/ebs"
    refresh                = false

    /* Optional Parameters */
    ami                    = "red_hat_enterprise_linux_8"
    instance_type          = "m6i.xlarge"
    set_state              = "running"


    # AWS AMI Module doesn't seem to like this being a variable, more testing needed.
    # !Error: Invalid Expression - A static list expression is required.
    # lifecycle = {
    #   ignore_changes = []
    # }

    # Operating System Volume
    root_block_device = {
      volume_type           = "gp3"
      volume_size           = "100"
      delete_on_termination = true
      tags = {}
    }

    network_interfaces = [
      {
        description     = "Primary Network Interface"
        private_ip      = "1.2.3.4"
        security_groups = [
            "sg-000a000aa000aaaaa",
            "sg-0aaa00a0a000a0000"
        ]
      }
    ]

    ebs_block_devices = [
      {
        final_snapshot        = true
        volume_type           = "gp3"
        volume_size           = "25"
        delete_on_termination = false
      }
    ]

    tags = {
      #Name        = <Set Automatically From 'hostname'>
      #OS          = <Set Automatically From 'ami'>
      Backup       = true
      Function     = "Jenkins Server"
      #Terraform   = <Statically Set To 'True'>
      #Environment = <Set Automatically From 'var.environment'>
    }
  }
]
